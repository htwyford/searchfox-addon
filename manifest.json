{
  "name": "Searchfox Search Engine",
  "manifest_version": 2,
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "searchfox-search-engine@harrytwyford.com"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.png"
  },
  "web_accessible_resources": [
    "favicon.png"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Searchfox",
      "keyword": "@searchfox",
      "search_url": "https://searchfox.org/mozilla-central/search",
      "search_url_get_params": "q={searchTerms}"
    }
  }
}
